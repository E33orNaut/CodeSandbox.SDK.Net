{
  "metadata": [
    {
      "src": [
        {
          "files": [ "CodeSandbox.SDK.Net.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ]
        }
      ],
      "dest": "obj/api",
      "properties": {
        "TargetFramework": "net47"
      }
    },
    {
      "src": [
        {
          "files": [ "Docs/CodeSandbox.SDK.Net.xml" ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      { "files": [ "api/**.yml" ] },
      { "files": [ "articles/**.md" ], "exclude": [ "**/obj/**" ] },
      { "files": [ "index.md" ] }
    ],
    "resource": [
      { "files": [ "images/**" ] }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "CodeSandbox.SDK.Net Documentation"
    },
    "template": [ "default" ]
  }
}
